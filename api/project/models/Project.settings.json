{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "user": {
      "model": "user",
      "via": "projects",
      "plugin": "users-permissions"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "artist": {
      "required": true,
      "type": "string"
    },
    "title": {
      "required": true,
      "type": "string"
    },
    "updated_at": {
      "type": "date"
    }
  }
}